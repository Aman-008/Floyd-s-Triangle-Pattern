#include <iostream>
using namespace std;

int main()
{
    int num , col , row;
    cin >> num;
    int c = 1;
    for ( row = 0; row < num; row++)
    {
        for (col = 0; col < row + 1; col++)
        {
            cout << c;
            c++;
        }
        cout << endl;
        
    }
    
    return 0;
}
